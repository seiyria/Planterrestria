{
  "name": "Planterrestria",
  "version": "0.0.0",
  "license": "GPL-3.0",
  "scripts": {
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "test:lint": "tslint --force src/**/*.ts",
    "test:lint:fail": "tslint src/**/*.ts",
    "test": "npm run lint:lint:fail",
    "docs": "typedoc --options typedoc.json src/client/app/app.component.ts",
    "start": "npm run start:server",
    "start:server": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/server/index.ts",
    "start:client": "webpack-dev-server --open --inline --hot --port 9090"
  },
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "2.1.2",
    "@angular/http": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/router": "3.1.2",
    "core-js": "^2.4.1",
    "lodash": "^4.17.1",
    "primus": "^6.0.5",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "@angularclass/hmr": "^1.0.1",
    "@angularclass/hmr-loader": "^3.0.2",
    "@types/core-js": "^0.9.0",
    "@types/lodash": "^4.14.39",
    "@types/node": "^6.0.38",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.3.2",
    "awesome-typescript-loader": "^2.2.4",
    "codelyzer": "1.0.0-beta.3",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.8.1",
    "json-loader": "^0.5.3",
    "node-sass": "^3.4.2",
    "nodemon": "^1.11.0",
    "null-loader": "0.1.1",
    "postcss-loader": "^1.1.0",
    "raw-loader": "0.5.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^4.0.0",
    "shelljs": "^0.7.0",
    "style-loader": "^0.13.0",
    "ts-helpers": "^1.1.1",
    "ts-node": "^1.7.0",
    "tslint": "^3.4.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.5.1",
    "typescript": "2.0.6",
    "url-loader": "^0.5.6",
    "webpack": "^2.1.0-beta.25",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "2.1.0-beta.9"
  }
}

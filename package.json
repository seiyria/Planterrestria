{
  "name": "Planterrestria",
  "version": "0.0.0",
  "license": "GPL-3.0",
  "scripts": {
    "build:app": "webpack -p --progress && cp src/client/public/index.html dist/index.html && echo '/*    /index.html   200' > dist/_redirects",
    "test:lint": "tslint --force 'src/**/*.ts'",
    "test:lint:fail": "tslint 'src/**/*.ts'",
    "test": "npm run test:lint:fail",
    "docs": "typedoc --options typedoc.json src/client/app/app.component.ts",
    "start": "npm run start:server",
    "start:server": "nodemon --exec \"ts-node\" -- \"src/server/index.ts\"",
    "start:client": "webpack-dev-server --open --inline --hot --port 9090 --host 0.0.0.0"
  },
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
